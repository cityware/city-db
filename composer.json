{
    "name": "cityware/city-db",
    "description": "Cityware Database",
    "license": "GPL-3.0",
    "keywords": [
        "cityware",
        "db"
    ],
    "authors": [
        {
            "name": "Fabricio S V Xavier",
            "email": "fabricio@cityware.com.br",
            "homepage": "http://www.cityware.com.br",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cityware/city-db.git"
        }
    ],
    "homepage": "https://github.com/cityware/city-db",
    "autoload": {
        "psr-4": {
            "Cityware\\Db\\": "src/"
        },
        "psr-0": {
            "Cityware\\Db\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "require": {
        "php": ">=5.3.23"
    },
    "require-dev": {
        "zendframework/zend-servicemanager": "~2.5",
        "fabpot/php-cs-fixer": "1.7.*",
        "phpunit/PHPUnit": "~4.0"
    },
    "suggest": {
        "ext-intl": "Required for most features of Zend\\I18n; included in default builds of PHP",
        "zendframework/zend-config": "Zend\\Config component",
        "zendframework/zend-db": "Zend\\Db component"
    },
    "minimum-stability":"dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CitywareTest\\Db\\": "test/"
        }
    }
}